%% 准备测试和机械臂之间的控制的通信
pnet('closeall');
clc;
clear;
close all;
%% Establish RobotControl comm.
global RobotControl 
RobotControl = tcpip('loaca', 5288, 'NetworkRole','client');
fopen(RobotControl);

RandomScene = [0,1,2];
pause(1);

for i=1:length(RandomScene)
    pause(1);
    disp(['Send message: ', num2str(RandomScene(i))]);
    switch RandomScene(i)
        case 2
            textSend='Y1';
            pause(0.1);
            fwrite(RobotControl, textSend);
        case 3
            textSend='Y2';
            pasue(0.1);
            fwrite(RobotControl, textSend);
        case 4
            textsend='Y3';
            pause(0.1);
            fwrite(RobotControl, textsend);
    end
end